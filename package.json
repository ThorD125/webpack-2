    "debugwin": "rmdir /S /Q dist && mkdir dist && webpack --stats-children --mode production",
    "debugmac": "rm -rf dist/* && webpack --stats-children --mode production",
    "buildmac": "rm -rf dist/* && webpack --mode production",
    "buildwin": "rmdir /S /Q dist && mkdir dist && webpack --mode production"
